// Sub-string divisibility

import std.stdio;
import digit;

void main() {
	long sum;
	auto nums = enumPandigital(10);
	foreach (_, n; nums) {
		if ((((((n / 100000000) % 10) * 100 +  
		       ((n / 10000000) % 10) * 10 +  
		       ((n / 1000000) % 10) * 1) % 2) == 0) &&
		    (((((n / 10000000) % 10) * 100 +  
		       ((n / 1000000) % 10) * 10 +  
		       ((n / 100000) % 10) * 1) % 3) == 0) &&
		    (((((n / 1000000) % 10) * 100 +  
		       ((n / 100000) % 10) * 10 +  
		       ((n / 10000) % 10) * 1) % 5) == 0) &&
		    (((((n / 100000) % 10) * 100 +  
		       ((n / 10000) % 10) * 10 +  
		       ((n / 1000) % 10) * 1) % 7) == 0) &&
		    (((((n / 10000) % 10) * 100 +  
		       ((n / 1000) % 10) * 10 +  
		       ((n / 100) % 10) * 1) % 11) == 0) &&
		    (((((n / 1000) % 10) * 100 +  
		       ((n / 100) % 10) * 10 +  
		       ((n / 10) % 10) * 1) % 13) == 0) &&
		    (((((n / 100) % 10) * 100 +  
		       ((n / 10) % 10) * 10 +  
		       ((n / 1) % 10) * 1) % 17) == 0)) {
			sum += n;
		}
	}
	writeln(sum);
}

